{
    "name": "gyselroth/balloon",
    "description": "fast and simple DMS",
    "keywords": ["dms", "nosql", "gui", "webdav", "filesystem", "vfs", "api", "secure", "fast"],
    "homepage": "http://www.github.com/gyselroth/balloon",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Balloon\\": "src/lib",
            "Balloon\\Testsuite\\": "tests"
        }
    },
    "repositories": [
         {
             "type": "git",
             "url": "https://github.com/raffis/php-mongomock"
         }
    ],
    "require": {
        "php": ">=7.1",
        "gyselroth/micro-auth": "*",
        "gyselroth/micro-container": "^1.0.0",
        "gyselroth/micro-http": "^0.0.6",
        "elasticsearch/elasticsearch": "5.*",
        "xenolope/quahog": "2.*",
        "sabre/dav" : "3.*",
        "psr/log": "1.*",
        "psr/container": "*",
        "mongodb/mongodb": "1.2.0",
        "ext-mongodb": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-intl": "*",
        "ulrichsg/getopt-php": "*",
        "zendframework/zend-mail": "2.7.*",
        "gyselroth/mongodb-php-task-scheduler": "^1.0.1",
        "hassankhan/config": "*",
        "symfony/yaml": "^4.0",
        "monolog/monolog": "^1.23",
        "bramus/monolog-colored-line-formatter": "~2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "maennchen/zipstream-php": "^0.5.1"
    },
    "suggest": {
        "ext-apc": "Allows caching configuration which gives more performance",
        "ext-imagick": "Required for image previews and converter",
        "ext-ldap": "Required for ldap authentication and ldap synchronization"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.0",
        "friendsofphp/php-cs-fixer": "*",
        "helmich/mongomock": "dev-master",
        "phpstan/phpstan": "*"
    }
}
