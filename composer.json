{
    "name": "gyselroth/balloon",
    "description": "fast and simple DMS",
    "keywords": ["dms", "nosql", "gui", "webdav", "filesystem", "vfs", "api", "secure", "fast"],
    "homepage": "http://www.github.com/gyselroth/balloon",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Balloon\\": "src/lib",
            "Balloon\\Testsuite\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/raffis/php-mongomock"
        },
        {
            "type": "git",
            "url": "https://github.com/raffis/SMB"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "gyselroth/micro-auth": "^1.0.0-alpha5",
        "gyselroth/micro-container": "^2.1.0",
        "gyselroth/micro-http": "^0.0.7",
        "elasticsearch/elasticsearch": "^6.1.0",
        "xenolope/quahog": "2.*",
        "sabre/dav" : "^3.2.0",
        "psr/log": "^1.0.0",
        "psr/container": "^1.0.0",
        "psr/simple-cache": "^1.0.0",
        "mongodb/mongodb": "^1.4.0",
        "ext-mongodb": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-intl": "*",
        "ulrichsg/getopt-php": "^3.2.0",
        "zendframework/zend-mail": "2.9.*",
        "gyselroth/mongodb-php-task-scheduler": "^3.1.0",
        "hassankhan/config": "^1.1.0",
        "symfony/yaml": "^4.0",
        "monolog/monolog": "^2.0.0-beta1",
        "bramus/monolog-colored-line-formatter": "~3.0",
        "zendframework/zend-servicemanager": "^3.4",
        "maennchen/zipstream-php": "^0.5.1",
        "katzien/php-mime-type": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "icewind/smb": "dev-both",
        "paragonie/halite": "^4",
        "bshaffer/oauth2-server-php": "^1.11",
        "dolondro/google-authenticator": "^2.0",
        "erusev/parsedown": "^1.7",
        "google/recaptcha": "^1.2",
        "cache/apcu-adapter": "^1.0",
        "phpseclib/phpseclib": "^2.0",
        "desean1625/structphp": "^0.9012.0",
        "web-auth/webauthn-lib": "^1.2",
        "zendframework/zend-diactoros": "^2.1",
        "web-token/jwt-framework": "^1.3",
        "php-http/discovery": "^1.6",
        "psr/http-client": "^1.0",
        "mjelamanov/psr18-guzzle": "^1.0",
        "vanilla/garden-schema": "^2.2",
        "mindplay/middleman": "^3.0",
        "middlewares/negotiation": "^1.1",
        "middlewares/payload": "^2.1",
        "middlewares/utils": "^2.1",
        "middlewares/trailing-slash": "^1.0",
        "middlewares/access-log": "^1.0",
        "nikic/fast-route": "^1.3",
        "middlewares/fast-route": "^1.0",
        "php-jsonpatch/php-jsonpatch": "^3.0",
        "stolt/json-merge-patch": "^1.0",
        "psr/http-message": "^1.0",
        "gyselroth/stream-iterator": "dev-master",
        "lcobucci/content-negotiation-middleware": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "zendframework/zend-httphandlerrunner": "^1.1",
        "league/event": "^2.2"
    },
    "suggest": {
        "ext-apc": "Allows caching configuration which gives more performance",
        "ext-imagick": "Required for image previews and converter",
        "ext-ldap": "Required for ldap authentication and ldap synchronization",
        "ext-smb": "Required for SMB mounts"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.0",
        "friendsofphp/php-cs-fixer": "*",
        "helmich/mongomock": "dev-master#1ee4147",
        "phpstan/phpstan": "*"
    }
}
