{
  "name": "gyselroth/balloon",
  "description": "fast and simple DMS",
  "keywords": [
    "dms",
    "nosql",
    "gui",
    "webdav",
    "filesystem",
    "vfs",
    "api",
    "secure",
    "fast"
  ],
  "homepage": "http://www.github.com/gyselroth/balloon",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Raffael Sahli",
      "email": "sahli@gyselroth.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Balloon\\": "src/lib",
      "Balloon\\Testsuite\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/raffis/php-mongomock"
    },
    {
      "type": "git",
      "url": "https://github.com/raffis/SMB"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "gyselroth/micro-auth": "^0.0.11",
    "gyselroth/micro-container": "^2.3.3",
    "gyselroth/micro-http": "^0.0.8",
    "elasticsearch/elasticsearch": "^7.9",
    "xenolope/quahog": "2.*",
    "sabre/dav": "^4.0.0",
    "psr/log": "^1.0.0",
    "psr/container": "^1.0.0",
    "psr/simple-cache": "^1.0.0",
    "mongodb/mongodb": "^1.5.0",
    "ext-mongodb": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-zip": "*",
    "ext-intl": "*",
    "ext-smd5": "*",
    "ext-sockets": "*",
    "ulrichsg/getopt-php": "^3.2.0",
    "zendframework/zend-mail": "2.9.*",
    "gyselroth/mongodb-php-task-scheduler": "^4.0.15",
    "hassankhan/config": "^1.1.0",
    "symfony/yaml": "^4.0",
    "monolog/monolog": "^2.0.0-beta1",
    "bramus/monolog-colored-line-formatter": "~3.0",
    "zendframework/zend-servicemanager": "^3.3",
    "maennchen/zipstream-php": "^0.5.1",
    "katzien/php-mime-type": "^2.1",
    "guzzlehttp/guzzle": "^6.3",
    "icewind/smb": "dev-both",
    "paragonie/halite": "v4.6.0",
    "bshaffer/oauth2-server-php": "^1.11",
    "dolondro/google-authenticator": "^2.0",
    "erusev/parsedown": "^1.7",
    "google/recaptcha": "^1.2",
    "cache/apcu-adapter": "^1.0",
    "phpseclib/phpseclib": "^2.0",
    "desean1625/structphp": "^0.9012.0",
    "web-auth/webauthn-lib": "^1.2",
    "zendframework/zend-diactoros": "^2.1",
    "web-token/jwt-framework": "^1.3",
    "php-http/discovery": "^1.6",
    "mjelamanov/psr18-guzzle": "^1.0"
  },
  "suggest": {
    "ext-apc": "Allows caching configuration which gives more performance",
    "ext-imagick": "Required for image previews and converter",
    "ext-ldap": "Required for ldap authentication and ldap synchronization",
    "ext-smb": "Required for SMB mounts"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.0",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "helmich/mongomock": "dev-master#7bb0b5d",
    "phpstan/phpstan": "^1.8",
    "ddelnano/dredd-hooks-php": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
