{
    "name": "gyselroth/balloon",
    "description": "fast and simple DMS",
    "keywords": ["dms", "nosql", "gui", "webdav", "filesystem", "vfs", "api", "secure", "fast"],
    "homepage": "http://www.github.com/gyselroth/balloon",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Balloon\\": "src/lib",
            "Balloon\\Testsuite\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/raffis/php-mongomock"
        },
        {
            "type": "git",
            "url": "https://github.com/raffis/SMB"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "gyselroth/micro-auth": "^0.0.8",
        "gyselroth/micro-container": "^2.0.0",
        "gyselroth/micro-http": "^0.0.6",
        "elasticsearch/elasticsearch": "5.*",
        "xenolope/quahog": "2.*",
        "sabre/dav" : "3.*",
        "psr/log": "1.*",
        "psr/container": "*",
        "mongodb/mongodb": "^1.4.0",
        "ext-mongodb": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-intl": "*",
        "ulrichsg/getopt-php": "*",
        "zendframework/zend-mail": "2.9.*",
        "gyselroth/mongodb-php-task-scheduler": "^3.0.0",
        "hassankhan/config": "*",
        "symfony/yaml": "^4.0",
        "monolog/monolog": "^1.23",
        "bramus/monolog-colored-line-formatter": "~2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "maennchen/zipstream-php": "^0.5.1",
        "katzien/php-mime-type": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "icewind/smb": "dev-master#523d1bc",
        "paragonie/halite": "^4",
        "php-ffmpeg/php-ffmpeg": "^0.13.0"
    },
    "suggest": {
        "ext-apc": "Allows caching configuration which gives more performance",
        "ext-imagick": "Required for image previews and converter",
        "ext-ldap": "Required for ldap authentication and ldap synchronization",
        "ext-smb": "Required for SMB mounts"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.0",
        "friendsofphp/php-cs-fixer": "*",
        "helmich/mongomock": "dev-master#1ee4147",
        "phpstan/phpstan": "*"
    }
}
